        </activity>
        <activity android:name=".dashboardActivity"/>
        <activity android:name=".FormActivity"/>
        <activity android:name=".laptop"/>
        <activity android:name=".mobile"/>
        <activity android:name=".cars"/>
    </application>

</manifest>